P_0x5607eca4dcd0 .param/l "VOLUME_TOGGLE" 1 3 8, C4<0000011111>;
P_0x5607eca4dd10 .param/l "WARBLE_MAX" 1 3 7, +C4<00000101111101011110000100000000>;
v0x5607eca4e320_0 .net "clk_in", 0 0, v0x5607eca706e0_0;  1 drivers
