E_0x5607eca49e30 .event posedge, v0x5607eca4e320_0;
    .scope S_0x5607eca122c0;
T_0 ;
    %pushi/vec4 31, 0, 10;
    %store/vec4 v0x5607eca70210_0, 0, 10;
    %pushi/vec4 385280, 0, 20;
    %store/vec4 v0x5607eca70580_0, 0, 20;
    %end;
    .thread T_0;
    .scope S_0x5607eca122c0;
T_1 ;
    %wait E_0x5607eca49e30;
    %load/vec4 v0x5607eca70580_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_1.0, 4;
    %pushi/vec4 385280, 0, 20;
    %assign/vec4 v0x5607eca70580_0, 0;
    %load/vec4 v0x5607eca70210_0;
    %inv;
    %assign/vec4 v0x5607eca70210_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x5607eca70580_0;
    %subi 1, 0, 20;
    %assign/vec4 v0x5607eca70580_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5607eca122c0;
T_2 ;
    %wait E_0x5607eca49e30;
    %load/vec4 v0x5607eca70210_0;
    %addi 1, 0, 10;
    %assign/vec4 v0x5607eca70210_0, 0;
    %load/vec4 v0x5607eca702f0_0;
    %load/vec4 v0x5607eca70210_0;
    %cmp/u;
    %jmp/0xz  T_2.0, 5;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5607eca70470_0, 0;
    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v0x5607eca703b0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.2, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5607eca70470_0, 0;
T_2.2 ;
T_2.1 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5607eca12140;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5607eca706e0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5607eca70780_0, 0, 1;
    %end;
