v0x5607eca70780_0 .var "en", 0 0;
v0x5607eca70850_0 .net "pwm", 0 0, v0x5607eca70470_0;  1 drivers
S_0x5607eca122c0 .scope module, "sc1" "siren_controller" 2 7, 3 1 0, S_0x5607eca12140;
