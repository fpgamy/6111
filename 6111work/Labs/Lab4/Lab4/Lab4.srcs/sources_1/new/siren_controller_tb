#! /usr/bin/vvp
:ivl_version "10.2 (stable)" "(v10_2)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x56298fc24110 .scope module, "siren_controller_tb" "siren_controller_tb" 2 1;
 .timescale 0 0;
v0x56298fc839a0_0 .var "clk", 0 0;
v0x56298fc83a40_0 .var "en", 0 0;
v0x56298fc83b10_0 .net "pwm", 0 0, v0x56298fc83730_0;  1 drivers
S_0x56298fc24290 .scope module, "sc1" "siren_controller" 2 7, 3 1 0, S_0x56298fc24110;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk_in"
    .port_info 1 /INPUT 1 "en_in"
    .port_info 2 /OUTPUT 1 "pwm_out"
P_0x56298fc608e0 .param/l "VOLUME_TOGGLE" 1 3 8, C4<0000011111>;
P_0x56298fc60920 .param/l "WARBLE_MAX" 1 3 7, +C4<00000101111101011110000100000000>;
v0x56298fc60f30_0 .net "clk_in", 0 0, v0x56298fc839a0_0;  1 drivers
v0x56298fc834d0_0 .var "count", 9 0;
v0x56298fc835b0_0 .var "d", 9 0;
v0x56298fc83670_0 .net "en_in", 0 0, v0x56298fc83a40_0;  1 drivers
v0x56298fc83730_0 .var "pwm_out", 0 0;
v0x56298fc83840_0 .var "warble_counter", 19 0;
E_0x56298fc5bdf0 .event posedge, v0x56298fc60f30_0;
    .scope S_0x56298fc24290;
T_0 ;
    %pushi/vec4 31, 0, 10;
    %store/vec4 v0x56298fc835b0_0, 0, 10;
    %pushi/vec4 31, 0, 10;
    %store/vec4 v0x56298fc834d0_0, 0, 10;
    %pushi/vec4 385280, 0, 20;
    %store/vec4 v0x56298fc83840_0, 0, 20;
    %end;
    .thread T_0;
    .scope S_0x56298fc24290;
T_1 ;
    %wait E_0x56298fc5bdf0;
    %load/vec4 v0x56298fc83840_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_1.0, 4;
    %pushi/vec4 385280, 0, 20;
    %assign/vec4 v0x56298fc83840_0, 0;
    %load/vec4 v0x56298fc835b0_0;
    %inv;
    %assign/vec4 v0x56298fc835b0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x56298fc83840_0;
    %subi 1, 0, 20;
    %assign/vec4 v0x56298fc83840_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x56298fc24290;
T_2 ;
    %wait E_0x56298fc5bdf0;
    %load/vec4 v0x56298fc834d0_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_2.0, 4;
    %pushi/vec4 1023, 0, 10;
    %assign/vec4 v0x56298fc834d0_0, 0;
    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v0x56298fc834d0_0;
    %subi 1, 0, 10;
    %assign/vec4 v0x56298fc834d0_0, 0;
T_2.1 ;
    %load/vec4 v0x56298fc835b0_0;
    %load/vec4 v0x56298fc834d0_0;
    %cmp/u;
    %jmp/0xz  T_2.2, 5;
    %load/vec4 v0x56298fc83670_0;
    %assign/vec4 v0x56298fc83730_0, 0;
    %jmp T_2.3;
T_2.2 ;
    %load/vec4 v0x56298fc83670_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.4, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x56298fc83730_0, 0;
T_2.4 ;
T_2.3 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x56298fc24110;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56298fc839a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56298fc83a40_0, 0, 1;
    %end;
    .thread T_3;
    .scope S_0x56298fc24110;
T_4 ;
    %vpi_call 2 15 "$dumpfile", "siren_test.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x56298fc24110 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56298fc83a40_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56298fc83a40_0, 0, 1;
    %delay 100000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56298fc83a40_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56298fc83a40_0, 0, 1;
    %delay 5000000, 0;
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x56298fc24110;
T_5 ;
    %delay 5, 0;
    %load/vec4 v0x56298fc839a0_0;
    %inv;
    %store/vec4 v0x56298fc839a0_0, 0, 1;
    %jmp T_5;
    .thread T_5;
# The file index is used to find the file name in the following table.
