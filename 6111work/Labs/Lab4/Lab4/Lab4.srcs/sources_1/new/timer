S_0x561c11843110 .scope module, "timer_tb" "timer_tb" 2 1;
 .timescale 0 0;
v0x561c118a1870_0 .var "clk", 0 0;
v0x561c118a1930_0 .net "expired", 0 0, v0x561c118a14a0_0;  1 drivers
v0x561c118a1a00_0 .var "start", 0 0;
v0x561c118a1b00_0 .var "value", 3 0;
S_0x561c11843290 .scope module, "t1" "timer" 2 7, 3 1 0, S_0x561c11843110;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk_1hz_in"
    .port_info 1 /INPUT 1 "start_in"
    .port_info 2 /INPUT 4 "value_in"
    .port_info 3 /OUTPUT 1 "expire_out"
v0x561c1187a920_0 .net "clk_1hz_in", 0 0, v0x561c118a1870_0;  1 drivers
v0x561c118a13c0_0 .var "counter", 3 0;
v0x561c118a14a0_0 .var "expire_out", 0 0;
v0x561c118a1540_0 .net "start_in", 0 0, v0x561c118a1a00_0;  1 drivers
v0x561c118a1600_0 .var "started", 0 0;
v0x561c118a1710_0 .net "value_in", 3 0, v0x561c118a1b00_0;  1 drivers
E_0x561c1187b660 .event posedge, v0x561c118a1540_0, v0x561c1187a920_0;
    .scope S_0x561c11843290;
T_0 ;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x561c118a13c0_0, 0, 4;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561c118a1600_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x561c11843290;
T_1 ;
    %wait E_0x561c1187b660;
    %load/vec4 v0x561c118a1540_0;
    %load/vec4 v0x561c118a1600_0;
    %inv;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x561c118a1600_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x561c118a14a0_0, 0;
    %load/vec4 v0x561c118a1710_0;
    %assign/vec4 v0x561c118a13c0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x561c118a13c0_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_1.2, 4;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x561c118a1600_0, 0;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x561c118a13c0_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x561c118a14a0_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v0x561c118a13c0_0;
    %subi 1, 0, 4;
    %assign/vec4 v0x561c118a13c0_0, 0;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x561c11843110;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561c118a1870_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561c118a1a00_0, 0, 1;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x561c118a1b00_0, 0, 4;
    %end;
    .thread T_2;
    .scope S_0x561c11843110;
T_3 ;
    %vpi_call 2 11 "$dumpfile", "test.vcd" {0 0 0};
    %vpi_call 2 12 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x561c11843110 {0 0 0};
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x561c118a1b00_0, 0, 4;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561c118a1a00_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x561c118a1b00_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x561c118a1a00_0, 0, 1;
    %delay 500, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561c118a1a00_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x561c118a1a00_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 24 "$finish" {0 0 0};
    %end;
